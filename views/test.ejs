    <section id = sec4>
        <div class="container">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Date</th>
                        <th>Comment</th>
                    </tr>
                </thead>
                <tbody id="postTable">
                    <% for(var i = 0; i < post.length; i++) {
                        %>
                    <tr id="<%= post[i]._id %>">
                        <td><%= post[i].name %> </td>
                        <td><%= post[i].date %> </td>
                        <td><%= post[i].comment %> </td>

                       
                        <button type="button" class="btn btn-warning deleteTest">Delete Test</button></td>
                    </tr>
                    <%}%>
                    
                </tbody>
            </table>
        </div>
    </section> <!--end sec4 -->

    <section id= secform>
            <div class="container center">
                <div class="row">
                    <div class="col-md-6">
                        <form class="form-inline" action = "/api/post" method = "POST" role="form"> 
                        
                            <div class="form-group">
                                <label class="sr-only" for="">label</label>
                                <input required name="name" type="text" class="form-control" id="name" placeholder="name">
                            </div> 
                            <div class="form-group">
                                <label name= "date" class="sr-only" for="">label</label>
                                <input required type="text" name="date" class="form-control" id="date" placeholder="date">
                            </div>

                            <div class="form-group">
                                <label name= "comment" class="sr-only" for="">label</label>
                                <input required type="text" name="comment" class="form-control" id="comment" placeholder="comment">
                            </div>                         
                            <button type="submit" class="btn btn-primary">Add Test</button>        
                        </form>
                    </div>
                </div>
            
            </div>
    </section> <!-- end secform .-->
